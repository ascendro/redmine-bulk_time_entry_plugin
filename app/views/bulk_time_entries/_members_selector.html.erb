<%= label_for_field :member_id, rnd %>
<% if @members.nil? || @members.empty? %>
<em><%= l(:label_insufficient_right) %></em>
<% else %>
<%= select_tag "time_entries[#{rnd}][member_id]", options_from_collection_for_select(@members, :id, :name, @members.find{|m| m.user == User.current}.id), :style => 'max-width: 95%;' %>&nbsp;
<% end %>
